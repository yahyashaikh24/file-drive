+		// when the event 'touchend' does not return the touch position
+
+		let touches = e.changedTouches
+
+		if (touches!=null&&touches.length>0) {
+			pos.x = touches[0].clientX
+			pos.y = touches[0].clientY
+		}
+
+	}
